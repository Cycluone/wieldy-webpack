  "license": "MIT",
  "dependencies": {
    "normalize.css": "^7.0.0",
    "vue": "^2.4.4"
  },
  "devDependencies": {
    "vue-loader": "^13.0.5",
    "vue-template-compiler": "^2.4.4",
    "webpack": "^3.3.0",
    "webpack-dev-server": "^2.6.1",
    "wieldy-webpack": "1.x"
  }
}
